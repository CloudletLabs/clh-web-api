{
  "$schema": "http://json-schema.org/draft-04/schema",
  "type": "object",
  "required": true,
  "properties": {
    "auth_token": {
      "type": "string",
      "required": true
    },
    "userAgent": {
      "type": "string",
      "required": true
    },
    "ip": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "object",
      "required": true,
      "properties": {
        "username": {
          "type": "string",
          "required": true
        },
        "roles": {
          "type": "array",
          "required": true,
          "items": {
            "type": "object",
            "properties": {
              "roleId": {
                "type": "string",
                "required": true
              }
            }
          }
        }
      }
    },
    "lastUsed": {
      "type": "string",
      "required": true
    },
    "createDate": {
      "type": "string",
      "required": true
    }
  }
}